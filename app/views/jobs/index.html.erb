<p id="notice"><%= notice %></p>

<h1>Jobs</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Priority</th>
      <th>Attempts</th>
      <th>Handler</th>
      <th>Last Error</th>
      <th>Run At</th>
      <th>Queue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.id %></td>
        <td><%= job.priority %></td>
        <td><%= job.attempts %></td>
        <td><%= job.handler %></td>
        <td><%= job.last_error %></td>
        <td><%= job.run_at %></td>
        <td><%= job.queue %></td>
        <td><%= link_to 'Show' %></td>
        <td></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>